<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Aprovar/Reprovar planos</h3>
  </div>
  <div class="panel-body">
    <div class="table-responsive">
      <%= content_tag :table, id: 'plans_datatable', class: 'table table-striped table-bordered', data: { url: get_planos_aprovar_path(format: :json)} do %>
        <thead>
          <tr>
            <th>Curso</th>
            <th>Disciplina</th>
            <th>Turma</th>
            <th>Professor</th>
            <th>Oferta</th>
            <th>Situação</th>
            <th>Ações</th>
          </tr>
        </thead>
        <tbody></tbody>
      <% end %>
    </div>

  </div>
</div>
