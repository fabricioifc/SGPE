<%= bootstrap_form_for(plan, local:true, html: { class: "", role: "form" }) do |f| %>
  <%= error_messages_for(plan) %>


      <!-- <= f.text_field :offer_discipline, id: :plan_offer_discipline %>
      <= f.text_field :plan_class, id: :plan_plan_class %> -->
      <%= f.select :offer_discipline, @ofertasProfessor.collect { |c| ["#{c.sigla} - #{c.name}", c.id] }, id: :plan_offer_discipline, prompt: true %>
      <%= f.text_area :obj_espe, id: :plan_obj_espe %>
      <%= f.text_area :conteudo_prog, id: :plan_conteudo_prog %>
      <%= f.text_area :prat_prof, id: :plan_prat_prof %>
      <%= f.text_area :interdisc, id: :plan_interdisc %>
      <%= f.text_area :met_tec, id: :plan_met_tec %>
      <%= f.text_area :met_met, id: :plan_met_met %>
      <%= f.text_area :avaliacao, id: :plan_avaliacao %>
      <%= f.text_area :cronograma, id: :plan_cronograma %>
      <%= f.text_area :atendimento, id: :plan_atendimento %>
      <%= f.number_field :versao, id: :plan_versao %>
      <%= f.check_box :active, id: :plan_active %>
      <%= f.text_field :user, id: :plan_user %>

  <%= f.submit class: "btn btn-primary" %>
<% end %>
