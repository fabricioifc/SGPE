<%- model = Plan %>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">
      <%= t('helpers.titles.new', model: model.model_name.human.downcase) %>
    </h3>
  </div>
  <div class="panel-body">
    <%= render 'form', plan: @plan, url: offer_offer_discipline_plans_path %>
  </div>
  <div class="panel-footer">
    <%= link_to t('helpers.links.back'), offer_offer_discipline_plans_path(offer_discipline_id: params[:offer_discipline_id]) %>
  </div>
</div>
