<p id="notice"><%= notice %></p>

<h1>Permissaos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Classe</th>
      <th>Acao</th>
      <th>Idativo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @permissaos.each do |permissao| %>
      <tr>
        <td><%= permissao.name %></td>
        <td><%= permissao.classe %></td>
        <td><%= permissao.acao %></td>
        <td><%= permissao.idativo %></td>
        <td><%= link_to 'Show', permissao %></td>
        <td><%= link_to 'Edit', edit_permissao_path(permissao) %></td>
        <td><%= link_to 'Destroy', permissao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Permissao', new_permissao_path %>
