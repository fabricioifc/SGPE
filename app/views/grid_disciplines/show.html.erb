<div class="page-header">
  <%= link_to grid_disciplines_path, class: 'btn btn-default' do %>
    <i class='fa fa-list fa-2'></i>
    <%= t('helpers.links.back') %>
  <% end %>
  <%= link_to edit_grid_discipline_path(@grid_discipline), class: 'btn btn-primary' do %>
    <i class='fa fa-pencil-square-o fa-2'></i>
    <%= t('helpers.links.edit') %>
  <% end %>
  <h1>Show grid_discipline</h1>
</div>

<dl class="dl-horizontal">
  <dt>Year:</dt>
  <dd><%= @grid_discipline.year %></dd>

  <dt>Ementa:</dt>
  <dd><%= @grid_discipline.ementa %></dd>

  <dt>Objetivo geral:</dt>
  <dd><%= @grid_discipline.objetivo_geral %></dd>

  <dt>Bib geral:</dt>
  <dd><%= @grid_discipline.bib_geral %></dd>

  <dt>Bib espec:</dt>
  <dd><%= @grid_discipline.bib_espec %></dd>

  <dt>Grid:</dt>
  <dd><%= @grid_discipline.grid %></dd>

  <dt>Discipline:</dt>
  <dd><%= @grid_discipline.discipline %></dd>

</dl>
