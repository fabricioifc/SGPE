<%%= bootstrap_form_for(<%= singular_table_name %>, local:true, html: { class: "", role: "form" }) do |f| %>
  <%%= error_messages_for(<%= singular_table_name %>) %>


  <% attributes.each do |attribute| -%>
    <%%= f.<%= attribute.field_type %> :<%= attribute.name %>, id: :<%= singular_table_name %>_<%= attribute.name %> %>
  <% end -%>

  <%%= f.submit class: "btn btn-primary" %>
<%% end %>
